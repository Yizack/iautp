<!-- NOMBRE -->
<category>
	<pattern>MI NOMBRE ES *</pattern>
	<template>
		<random>
			<li>Mucho gusto <set name="nombre"><star/></set>!, ¿En qué podemos ayudarte?</li>
			<li><set name="nombre"><star/></set>, me gusta tu nombre ¿Quieres hacerme unas preguntas?</li>
			<li>Lo recordaré.</li>
		</random>
		<think><set name="nombre"><star/></set></think>
	</template>
</category>
